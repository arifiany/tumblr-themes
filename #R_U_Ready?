    <script>
        $(document).ready(function() {
            $('a[href*="#"]:not([href="#verybigmenu"])').click(function() {
                if (location.pathname.replace(/^\//,'') == this.pathname.replace(/^\//,'') && location.hostname == this.hostname) {
                  var target = $(this.hash);
                  target = target.length ? target : $('[name=' + this.hash.slice(1) +']');
                  if (target.length) {
                    $('body').animate({
                      scrollTop: target.offset().top
                    }, 1000);
                    return false;
                  }
                }
              });
              
               $("body, #smalldesk, #bigdesk").niceScroll(
                   {
                       cursorcolor:"{color:second accent}",
                       autohidemode: "false",
                       bouncescroll: "true",
                       cursorborderradius: "0px",
                       cursorborder: "1px solid {color:first accent}"
                       
                   }
            );
        });
        
        

    </script>
